@model IEnumerable<WebApp5ByJessicaa.Models.Student>

<h2>Students List</h2>
<a asp-action="Create">Add New Student</a>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Age</th>
            <th>Address</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.Name</td>
                <td>@student.Age</td>
                <td>@student.Address</td>
            </tr>
        }
    </tbody>
</table>
